#include <stdio.h>

int main() {
    int n, i, j;

    printf("Enter a number: ");
    scanf("%d", &n);

    printf("Prime numbers between 1 and %d are:\n", n);

    // check every number from 2 to n
    for (i = 2; i <= n; i++) {
        int count = 0;   // to count divisors

        // check divisibility of i
        for (j = 1; j <= i; j++) {
            if (i % j == 0) {
                count++; // increase count if divisible
            }
        }

        // a prime number has exactly 2 divisors (1 and itself)
        if (count == 2) {
            printf("%d ", i);
        }
    }

    return 0;
}
